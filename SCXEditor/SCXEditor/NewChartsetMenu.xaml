<Window x:Class="SCXEditor.NewChartsetMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SCXEditor"
        mc:Ignorable="d"
        Title="New Chartset" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox Name="songTitleText">
                Song Title
            </TextBox>
            <TextBox Name="songArtistText">
                Song Artist
            </TextBox>
            <StackPanel Orientation="Horizontal">
                <TextBox Name="songAudioPathText">
                    Song Audio Path
                </TextBox>
                <Button Name="songAudioPathFolderSelectButton">
                    Select Path
                </Button>
            </StackPanel>
            <Button Name="newChartsetNextButton" Click="newChartsetNextButton_Click">Next</Button>
        </StackPanel>
    </Grid>
</Window>
